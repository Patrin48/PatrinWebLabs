@model System.Collections.ArrayList
  <div ud="SearchResults">
    <div class="row">
        <div class="col-sm-12 col-xs-12 col-md-12 col-lg-12" style="padding-top:25px">
            <div class="panel-group">
                <div class="panel panel-info">
                    <div class="panel-heading">Терминалы на линиях - проверка доступности</div>
                    <table class="table table-hover table-bordered" id="table3">
                        <thead>
                            <tr>
                                <th>Тип устройства</th>
                                <th>IP-адрес</th>
                                <th>Линия</th>
                                <th>Рабочее место</th>
                                <th>Статус</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                @foreach (var data in ((PatrinWebLabs.Models.Tablets)Model[0]).Data3)
                                {
                            <tbody>
                                <tr>
                                    <td><img src="http://stylus.ua/thumbs/320x320/94/26/425608.jpeg" class="img-thumbnail" alt="Планшет" width="50" height="50" /> Планшет</td>
                                    <td>@data.IP</td>
                                    <td>@data.LineTablet</td>
                                    <td>@data.NameWorkPlaceTablet</td>
                                    @if (@data.Tablets_Status == "В сети")
                                    {
                                        <td class="success">@data.Tablets_Status</td>
                                    }
                                    else
                                    {
                                        <td class="danger">@data.Tablets_Status</td>
                                    }
                                    <td>
                                        <label class="custom">
                                            <input type="checkbox" name="record"><span></span>
                                        </label>
                                    </td>
                                </tr>
                            </tbody>
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>